{
  "files.exclude": {
    "**/backups/**": true
  },
  "files.watcherExclude": {
    "**/backups/**": true,
    "**/node_modules/**": true
  },
  "search.exclude": {
    "**/backups/**": true,
    "**/node_modules/**": true
  },
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "java.import.gradle.offline.enabled": false,
  "java.import.gradle.excludedProjects": [
    "**/backups/**",
    "**/node_modules/**",
    "node_modules",
    "**/node_modules/**/android/**",
    "android",
    "**/android/**"
  ],
  "java.project.resourceFilters": [
    "**/backups/**",
    "**/node_modules/**",
    "node_modules",
    "**/node_modules/**/android/**",
    "android",
    "**/android/**"
  ],
  "java.project.importOnFirstTimeStartup": "disabled",
  "files.associations": {
    "**/backups/**/*.gradle": "plaintext",
    "**/backups/**/*.java": "plaintext",
    "**/backups/**/*.kt": "plaintext",
    "**/node_modules/**/*.gradle": "plaintext"
  },
  "java.compile.nullAnalysis.mode": "disabled",
  "gradle.nestedProjects": false,
  "gradle.autoDetect": "off",
  "problems.showCurrentInStatus": false,
  "problems.decorations.enabled": false,
  "gradle.excludedProjects": [
    "android",
    "**/node_modules/**"
  ]
}